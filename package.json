{
  "name": "@blackglory/static-js",
  "version": "0.2.8",
  "description": "",
  "keywords": [],
  "files": [
    "lib",
    "dist"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "sideEffects": false,
  "repository": "git@github.com:BlackGlory/static-js.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src __tests__",
    "test": "jest --no-cache --config jest.config.js",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:coverage": "jest --coverage --config jest.config.js",
    "prepublishOnly": "run-s clean build",
    "clean": "rimraf lib",
    "build": "run-s build:*",
    "build:compile": "tsc --project tsconfig.build.json --module commonjs --target es2018 --outDir lib",
    "build:patch": "tscpaths -p tsconfig.build.json -s ./src -o ./lib",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s lint build test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@peculiar/webcrypto": "^1.4.0",
    "@types/jest": "^27.4.1",
    "@types/ms": "^0.7.31",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "eslint": "^8.26.0",
    "husky": "4",
    "jest": "^27.5.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "standard-version": "^9.5.0",
    "ts-jest": "^27.1.4",
    "tscpaths": "^0.0.9",
    "tslib": "^2.4.0",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "@extra-memoize/memory-cache": "^0.2.4",
    "extra-compatible": "^0.1.2",
    "extra-memoize": "^0.9.1",
    "justypes": "^3.0.0",
    "ms": "^2.1.3",
    "url-operator": "^0.1.0"
  }
}
